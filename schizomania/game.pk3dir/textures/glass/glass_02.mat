{
    surfaceParm glass
    surfaceParm trans
    surfaceParm alphashadow
    cull none

    qer_editorImage "textures/glass/glass_01.tga"
    {
        program reflect
        map textures/glass/glass_01.tga
        map textures/glass/glass_01_normal.tga
        map $reflectcube
        blendFunc GL_ONE GL_ONE
    }
}
